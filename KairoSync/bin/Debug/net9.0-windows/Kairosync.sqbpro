CREATE TRIGGER &quot;GecikmeHesaplaGörevler&quot;
AFTER UPDATE OF &quot;TamamTarihi&quot; ON &quot;görevler&quot;
FOR EACH ROW
BEGIN
    UPDATE &quot;görevler&quot;
    SET &quot;Gecikme&quot; = CASE 
        WHEN NEW.&quot;TamamTarihi&quot; IS NOT NULL THEN 
            CASE 
                WHEN (JULIANDAY(NEW.&quot;TamamTarihi&quot;) - JULIANDAY(NEW.&quot;BitişTarihi&quot;)) &lt; 0 THEN NULL
                ELSE CAST((JULIANDAY(NEW.&quot;TamamTarihi&quot;) - JULIANDAY(NEW.&quot;BitişTarihi&quot;)) AS INTEGER)
            END
        ELSE 
            CASE 
                WHEN (JULIANDAY('now') - JULIANDAY(NEW.&quot;BitişTarihi&quot;)) &lt; 0 THEN NULL
                ELSE CAST((JULIANDAY('now') - JULIANDAY(NEW.&quot;BitişTarihi&quot;)) AS INTEGER)
            END
    END
    WHERE &quot;GörevID&quot; = NEW.&quot;GörevID&quot;;
END;
