  ProjeID INTEGER PRIMARY KEY AUTOINCREMENT,
  Ad TEXT NOT NULL,
  BaşlangıçTarihi DATETIME NOT NULL,
  BitişTarihi DATETIME DEFAULT NULL,
  Gecikme INTEGER DEFAULT 0,
  aciklama TEXT
);

CREATE TABLE çalışanlar (
  ÇalışanID INTEGER PRIMARY KEY AUTOINCREMENT,
  Ad TEXT NOT NULL,
  Soyad TEXT NOT NULL,
  Email TEXT NOT NULL UNIQUE,
  TelNo TEXT NOT NULL UNIQUE,
  DoğumTarihi DATE NOT NULL
);

CREATE TABLE görevler (
  GörevID INTEGER PRIMARY KEY AUTOINCREMENT,
  Ad TEXT NOT NULL,
  BaşlangıçTarihi DATETIME NOT NULL,
  BitişTarihi DATETIME DEFAULT NULL,
  AdamGün REAL NOT NULL,
  Durum TEXT CHECK(Durum IN ('Tamamlanacak', 'Devam Ediyor', 'Tamamlandı')) NOT NULL,
  ProjeID INTEGER DEFAULT NULL,
  ÇalışanID INTEGER DEFAULT NULL,
  FOREIGN KEY (ProjeID) REFERENCES projeler (ProjeID) ON DELETE CASCADE,
  FOREIGN KEY (ÇalışanID) REFERENCES çalışanlar (ÇalışanID) ON DELETE SET NULL
);
